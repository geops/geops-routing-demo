{
  "name": "geops-routing-demo",
  "description": "Sample component that uses geOps Routing API",
  "main": "build/index.js",
  "version": "1.0.0",
  "homepage": "https://routing-demo.geops.io/",
  "dependencies": {
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.39",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.1.4",
    "@testing-library/user-event": "14.4.3",
    "@turf/combine": "^6.0.1",
    "@turf/nearest-point-on-line": "^6.0.2",
    "abortcontroller-polyfill": "^1.2.1",
    "canvas": "^2.11.0",
    "css-mediaquery": "^0.1.2",
    "lodash": "^4.17.15",
    "mapbox-gl": "^1.7.0",
    "mobility-toolbox-js": "^1.3.9",
    "ol": "6.2.1",
    "prop-types": "^15.7.2",
    "react": "16.14.0",
    "react-app-polyfill": "2.0.0",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "16.14.0",
    "react-icons": "^4.2.0",
    "react-id-generator": "^3.0.0",
    "react-redux": "^7.1.3",
    "react-spatial": "1.0.30",
    "recharts": "2.1.4",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "sass": "^1.54.5"
  },
  "peerDependencies": {
    "react": "16.14.0",
    "react-dom": "16.14.0"
  },
  "devDependencies": {
    "better-docs": "^1.4.7",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "fixpack": "^2.3.1",
    "generact": "^0.4.0",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "jest-canvas-mock": "^2.2.0",
    "jest-serializer-html": "^7.1.0",
    "jsdoc": "^3.6.3",
    "lint-staged": "10.4.0",
    "prettier": "2.7.1",
    "react-scripts": "5.0.1",
    "react-test-renderer": "^16.12.0",
    "redux-mock-store": "^1.5.4",
    "style-loader": "^1.1.2",
    "stylelint": "13.13.1",
    "stylelint-config-recommended-scss": "4.3.0",
    "stylelint-config-standard": "22.0.0",
    "stylelint-scss": "3.21.0"
  },
  "scripts": {
    "build": "react-scripts build",
    "cp": "generact --root src/Components",
    "deploy:ghpages": "gh-pages -d build",
    "docs": "./node_modules/.bin/jsdoc -c jsdoc.json",
    "eject": "react-scripts eject",
    "format": "prettier --write 'src/**/*.{js,jsx}' && eslint 'src/**/*.{js,jsx}' --fix && stylelint 'src/**/*.css' 'src/**/*.scss' --fix",
    "lint": "eslint 'src/**/*.{js,jsx}' && stylelint 'src/**/*.{css,scss}'",
    "start": "ESLINT_NO_DEV_ERRORS=true  react-scripts start",
    "test": "react-scripts test  --env=jsdom --transformIgnorePatterns \"/node_modules/(?!(ol|react-spatial)/).*/\"",
    "up": "yarn upgrade-interactive --latest"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all",
    "not ie <= 11",
    "not android < 5"
  ],
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true,
      "es6": true,
      "jest": true
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "jsx": true,
      "impliedStrict": true
    },
    "extends": [
      "airbnb",
      "airbnb/hooks",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "linebreak-style": 0,
      "arrow-body-style": 0,
      "default-param-last": 0,
      "no-restricted-exports": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/forbid-prop-types": "Off",
      "prettier/prettier": "error"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "husky": {
    "hooks": {
      "post-checkout": "yarn install --frozen-lockfile",
      "post-merge": "yarn install --frozen-lockfile",
      "post-rebase": "yarn install --frozen-lockfile",
      "pre-commit": "CI=true lint-staged   "
    }
  },
  "jest": {
    "coverageReporters": [
      "text",
      "html"
    ],
    "snapshotSerializers": [
      "jest-serializer-html"
    ],
    "transformIgnorePatterns": []
  },
  "keywords": [
    "demo",
    "geops",
    "gis",
    "openlayers",
    "routing"
  ],
  "license": "MIT",
  "lint-staged": {
    "src/lang/*.json": [
      "yarn sort"
    ],
    "(src|__mocks__)/**/*.js": [
      "eslint --fix",
      "prettier --write",
      "yarn test --bail --findRelatedTests"
    ],
    "package.json": [
      "fixpack --sortToTop name --sortToTop description --sortToTop main --sortToTop version --sortToTop private --sortToTop homepage --sortToTop dependencies --sortToTop peerDependencies --sortToTop devDependencies --sortToTop scripts"
    ],
    "src/**/*.{css,scss}": [
      "stylelint --fix"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/geops/geops-routing-demo.git"
  },
  "stylelint": {
    "plugins": [
      "stylelint-scss"
    ],
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-recommended-scss"
    ],
    "rules": {
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "export"
          ]
        }
      ],
      "property-no-unknown": [
        true,
        {
          "ignoreProperties": [
            "/^brandprimary/",
            "/gray/"
          ]
        }
      ]
    }
  }
}
